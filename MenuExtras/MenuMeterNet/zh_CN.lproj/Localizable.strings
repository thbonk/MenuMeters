"AppleTalk:" = "AppleTalk：";

// Format string for AppleTalk display info
"Net: %@ Node: %@ Zone: %@" = "网络： %1$@ 节点： %2$@ 域： %3$@";

// Throughput menu title
"Throughput:" = "吞吐量：";

// Peak throughput menu title
"Peak Throughput:" = "尖峰吞吐量：";

// Traffic totals menu title
"Traffic Totals:" = "流量合计：";

